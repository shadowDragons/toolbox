        "uuid": "^10.0.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/lodash": "^4.17.7",
        "@types/micromatch": "^4.0.9",
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^8.2.0",
        "@typescript-eslint/parser": "^8.2.0",
        "autoprefixer": "^10.4.20",
        "cross-env": "^7.0.3",
        "eslint": "^8.57.0",
        "eslint-config-airbnb": "^19.0.4",

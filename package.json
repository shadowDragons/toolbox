    "prisma": {
        "schema": "src/database/schema",
        "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} -r tsconfig-paths/register src/database/seed/index.ts"
    },
    "pnpm": {
        "updateConfig": {
            "ignoreDependencies": [
                "eslint",
                "eslint-plugin-unused-imports"
            ]
        },
        "peerDependencyRules": {
            "allowAny": [
                "eslint",
                "@typescript-eslint/*"
            ]
        }
    }
}

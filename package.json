{
    "name": "nextapp",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "------------------ app command": "----",
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "------------------ lint command": "----",
        "lint": "pnpm lint:es && pnpm lint:style",
        "lint:es": "next lint --fix",
        "lint:style": "stylelint \"**/*.css\" --fix --cache --cache-location node_modules/.cache/stylelint/",
        "------------------ ui command": "----",
        "addsc": "pnpm dlx shadcn-ui@0.8.0 add -c ./src/app"
    },
    "dependencies": {
        "@faker-js/faker": "^8.4.1",
        "@hookform/resolvers": "^3.9.0",
